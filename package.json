{
  "name": "quiz-tina-turner",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "start": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "remove-dist": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production babel-node tools/build.js",
    "build:qlt": "cross-env INFO_ENV=qlt npm run build",
    "build:embed": "cross-env npm run build --embed",
    "build:epoca": "cross-env INFO_CHANNEL=epoca npm run build",
    "build:epoca:qlt": "cross-env INFO_CHANNEL=epoca npm run build:qlt",
    "check-npm-updates": "npm-check-updates",
    "update-packages": "npm-check-updates -u",
    "create": "node ./tools/templateCreator.js comp -c",
    "open:src": "cross-env NODE_ENV=development babel-node tools/srcServer.js",
    "lint:watch": "npm run lint -- --watch",
    "lint": "esw ./webpack/webpack.config.* src tools --color",
    "postbuild": "babel-node ./tools/formatHtml.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "7.14.6",
    "@babel/helper-compilation-targets": "^7.14.5",
    "@babel/node": "7.14.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@hot-loader/react-dom": "^17.0.1",
    "autoprefixer": "10.2.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.2",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "^2.27.11",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "concurrently": "6.2.0",
    "connect-history-api-fallback": "1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "7.28.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-watch": "7.0.0",
    "file-loader": "6.2.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "27.0.4",
    "jest-cli": "27.0.4",
    "npm-check-updates": "^11.7.1",
    "open-cli": "7.0.0",
    "prompt": "1.1.0",
    "prop-types": "15.7.2",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "raf": "3.4.1",
    "react-hot-loader": "4.13.0",
    "react-is": "^17.0.2",
    "replace": "^1.2.2",
    "rimraf": "3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "url-loader": "4.1.1",
    "webpack": "5.39.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-dev-middleware": "5.0.0",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "react"
  ]
}
